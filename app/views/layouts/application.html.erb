<!DOCTYPE html>
<html>
  <head>
    <title>DevopsApi</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= (button_to "logout", destroy_user_session_path, method: :delete) if current_user  %>
    <%= yield %>
  </body>
</html>
